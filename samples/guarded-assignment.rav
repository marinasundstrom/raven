import System.*
import System.Console.*

val res = Test(null)

WriteLine(res)

func Test(name: string?) -> Result<string, CustomError> {
    val str = name ?? {
        // Either act or return value

        return .Error(CustomError(nameof(name)))
    }

    WriteLine(str)
    
    .Ok(str)
}

record class CustomError(Message: string)