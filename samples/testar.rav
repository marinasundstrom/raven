
import System.*

val test: Result<int, string> = Ok(42)

val x = test match {
    Ok(42) => "Answer is 42"
    Ok(val no) => "Other no: $no"
    Error(val msg) => msg 
}

Console.WriteLine(x)

if test is Ok(42) {
    Console.WriteLine("42")
}

if test is Ok(val value) {
    Console.WriteLine("Value is: $value")
}