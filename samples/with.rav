import System.Console.*

val bob = Person("Bob", null, "Smith", 1978)
WriteLine(bob)

val bob2 = Person("Bob", null, "Smith", 1978)

WriteLine(bob2 == bob)

val newBob = bob with {
    MiddleName = "Leslie"
}

WriteLine(newBob)

val renamed = newBob with {
    MiddleName = "Flash",
    LastName = "Jones"
}

WriteLine(renamed)

record class Person(Name: string, MiddleName: string?, LastName: string, YearOfBirth: int)
