import System.*
import System.Console.*

func Main() {
    val fooTrue  = new Foo(true,  (10, 20))
    val fooFalse = new Foo(false, (1, 2))

    test(fooTrue)
    test(fooFalse)
}

func test(x: object?) {
    if x is Foo(true, (val a, val b)) {
        WriteLine("Foo with Value == true and $a and $b")
    }
}

record class Foo(Value: bool, Data: (int, int))