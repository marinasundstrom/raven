/*
 * Demonstrates discriminated unions in Raven.
 */

import System.*

val unwrapped3 = IntHelpers.ParseNumber("23").UnwrapOrThrow()
Console.WriteLine(unwrapped3)

public class IntHelpers {
    public static ParseNumber(str: string) -> Result<int> {
        return try int.Parse(str) match {
            int v => .Ok(v)
            Exception exc => .Error(exc.Message)
        }
    }
}
