import System.Console.*

val f: Foo? = Foo(21);
val s = f?(2)

WriteLine("Invocation operator: $s")

val s2 = f?.Hello()

WriteLine("Extension: $s2")

val s3 = Foo.Hello2()

WriteLine("Static extension: $s3")

public class Foo(b: int) {
    public self(factor: int) -> int {
        return b * factor
    }
}

public extension Test for Foo {
    public Hello() -> string {
        return self.ToString() 
    }

    public static Hello2() -> string {
        return "Foobar" 
    }
}