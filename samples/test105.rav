import System.*

val f = Foo()
f.Raised += (s, e) => Console.WriteLine("Hello")

val x = f.RaiseEvent(2)

Console.WriteLine(x)

public class Foo {
    public RaiseEvent(x: int) -> int {
        Raised?.Invoke(self, EventArgs.Empty)
        return 42
    }
    
    public event Raised: EventHandler?
}