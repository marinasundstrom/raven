import System.*

f("test")
f(42)
f(null) // Shouldnt work, but infers shim type Null as type

f<bool?>(null) // Call isnt emitted (just emits ldnull)

f2<bool?>(null) // Should not be possible because of not null constraint (just emits ldnull)

func f<T>(t: T) -> () {
    Console.WriteLine(t)
}

func f2<T>(t: T) -> ()
    where T: notnull {
    Console.WriteLine(t)
}