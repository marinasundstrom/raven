/*
 * Demonstrates discriminated unions in Raven.
 */

import System.*

val test: Result<int, string> = Ok(42)

Console.WriteLine(test)

val x = test match {
    Ok(val v) => v.ToString()
    Error(val msg) => msg 
}

Console.WriteLine(x.GetType())

/*
union Result<T> {
    Ok(value: T)
    Error(message: string)
}
*/