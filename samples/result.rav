/*
 * Demonstrates discriminated unions in Raven.
 */

import System.*

let test: Result<int> = .Ok(42)

Console.WriteLine(test)

let x = test match {
    .Ok(val) => val.ToString()
    .Error(msg) => msg 
}

Console.WriteLine(x.GetType())

/*
union Result<T> {
    Ok(value: T)
    Error(message: string)
}
*/