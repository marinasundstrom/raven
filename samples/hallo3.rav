import System.Console.*

val test = Derived()
test.Foo()
test.Bar(2)

val test2 = Derived2()
test2.Foo()
test2.Bar(2)

open abstract class Base {
    public virtual Foo() {
        WriteLine("Test")
    }

    public abstract Bar(x: int)
}

class Derived : Base {
    public override Bar(x: int) {}
}

class Derived2 : Base {
    public override Foo() {
        WriteLine("Test2")
    }

    public override Bar(x: int) {}
}