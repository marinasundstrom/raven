import System.*
import System.Console.*
import System.Linq.*

func Main(args: string[]) -> () {
    WriteLine(args.Length)

    val r = typeof(String)
    WriteLine(r)

    val members = r.GetMembers(.Public | .Static)

    val name = "Intern"

    for member in members.Where(x => x.Name.Equals(name)) {
        WriteLine(member.Name)
    }

    val result = members.FirstOrNone()

    if(result is Some(val member)) {
        WriteLine("First: $member")
    }
}