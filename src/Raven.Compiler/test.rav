/* Sample program */

import System;

Console.WriteLine("Input: ");

let str = Console.ReadLine();
let value = bool.Parse(str);

var message = if value {
    "Hello" + ", World!";
} else {
    "boo";
};

var i = 0;

while i < 10
{
    Console.WriteLine("Test: " + i.ToString());
    i = i + 1;
};
    
// Print result
Console.WriteLine(message);