/*
 * Generics - instantiation of generic type and accessing members
 * * * */

import System.Console.*
import System.Collections.Generic.List<>

let list = List<int>()
list.Add(2)
list.Add(3)

WriteLine(list.Count)

var i = 0

while i < list.Count {
    let item = list[i]
    WriteLine(item); i = i + 1
}
