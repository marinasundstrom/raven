/*
 * Enums and target-type member access
 * * * */

import System;

var foo : Grades = .B;
foo = .C;

let x : string = .Empty;

Console.WriteLine(foo);
Console.WriteLine(foo.GetType());

Console.WriteLine(Grades.B.GetType());

if foo == .C {
    Console.WriteLine("Foo");
}

enum Grades { A, B, C };

func f () -> Grades {
    return .B;
}