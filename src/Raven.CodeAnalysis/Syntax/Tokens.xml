  <TokenKind Name="MultiLineCommentTrivia" Text="/*" IsTrivia="true" />

  <TokenKind Name="Whitespace" Text=" " IsTrivia="true" />
  <TokenKind Name="TabToken" Text="\t" IsTrivia="true" />
  <TokenKind Name="LineFeedToken" Text="\n" IsTrivia="true" />
  <TokenKind Name="CarriageReturnToken" Text="\r" IsTrivia="true" />
  <TokenKind Name="CarriageReturnLineFeedToken" Text="\r\n" IsTrivia="true" />
  <TokenKind Name="NewLineToken" Text="\n" />
  <TokenKind Name="EndOfFileToken" Text="" />

