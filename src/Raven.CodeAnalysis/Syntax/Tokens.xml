  <TokenKind Name="WhitespaceTrivia" Text=" " IsTrivia="true" />
  <TokenKind Name="TabTrivia" Text="\t" IsTrivia="true" />
  <TokenKind Name="LineFeedTrivia" Text="\n" IsTrivia="true" />
